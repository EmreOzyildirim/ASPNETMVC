@model IEnumerable<EmreOzyildirimBlog.Models.Entities.FooterDeveloperName>

@if (Model.Count() == 0)
{
    <div class="footer-designed-by">
        Developed &amp; Designed by <a href="#" class="f-db-link">Emre Efendi</a>
    </div>
}
else
{
    foreach (var item in Model)
    {

        <div class="footer-designed-by">
            @(item.DevelopedAndDesignedText == null ? "Developed &amp; Designed by" : item.DevelopedAndDesignedText) <a href="@(item.DeveloperLink == null ? "#" : item.DeveloperLink)" class="f-db-link">@(item.DeveloperName == null ? "Emre Efendi" : item.DeveloperName)</a>
        </div>

    }
}
